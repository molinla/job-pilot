{
	"name": "one-step-to-job",
	"version": "1.0.0",
	"description": "An Electron application with Vue and TypeScript",
	"main": "./out/main/index.js",
	"author": "example.com",
	"homepage": "https://electron-vite.org",
	"scripts": {
		"format": "prettier --write .",
		"lint": "eslint --cache .",
		"typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
		"typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
		"typecheck": "npm run typecheck:node && npm run typecheck:web",
		"start": "electron-vite preview",
		"dev": "electron-vite dev",
		"dev:debug": "ELECTRON_ENABLE_LOGGING=true electron-vite dev",
		"dev:permissions": "node scripts/dev-permissions.js",
		"dev:reset-permissions": "node scripts/dev-permissions.js --reset",
		"fix:screen-capture": "node scripts/fix-screen-capture.js",
		"build": "electron-vite build",
		"build-with-type-check": "npm run typecheck && electron-vite build",
		"postinstall": "electron-builder install-app-deps",
		"build:unpack": "npm run build && electron-builder --dir",
		"build:win": "npm run build && electron-builder --win",
		"build:mac": "npm run build && electron-builder --mac",
		"build:mac:debug": "npm run build && ELECTRON_ENABLE_LOGGING=true electron-builder --mac",
		"build:mac:permissions": "npm run build:mac && node scripts/mac-permissions.js",
		"build:linux": "npm run build && electron-builder --linux"
	},
	"dependencies": {
		"@electron-toolkit/preload": "^3.0.1",
		"@electron-toolkit/utils": "^4.0.0",
		"@tailwindcss/vite": "^4.0.12",
		"@xenova/transformers": "^2.17.2",
		"axios": "^1.9.0",
		"echarts": "^5.6.0",
		"monaco-editor": "^0.52.2",
		"vue-monaco-next": "0.0.1-beta.16",
		"vue-router": "4"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@electron-toolkit/tsconfig": "^1.0.1",
		"@tailwindcss/typography": "^0.5.16",
		"@types/node": "^22.13.4",
		"@vitejs/plugin-vue": "^5.2.1",
		"electron": "^34.2.0",
		"electron-builder": "^25.1.8",
		"electron-vite": "^3.0.0",
		"postcss": "^8.5.3",
		"tailwindcss": "^4.0.12",
		"typescript": "^5.7.3",
		"vite": "^6.1.0",
		"vite-plugin-monaco-editor": "^1.1.0",
		"vue": "^3.5.13",
		"vue-tsc": "^2.2.2"
	}
}
